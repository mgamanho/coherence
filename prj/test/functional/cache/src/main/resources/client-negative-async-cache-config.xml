<cache-config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns="http://xmlns.oracle.com/coherence/coherence-cache-config"
              xsi:schemaLocation="http://xmlns.oracle.com/coherence/coherence-cache-config coherence-cache-config.xsd">

  <caching-scheme-mapping>
    <cache-mapping>
      <cache-name>remote*</cache-name>
      <scheme-name>example-remote</scheme-name>
    </cache-mapping>

    <cache-mapping>
      <cache-name>view-extend*</cache-name>
      <scheme-name>example-view-extend</scheme-name>
    </cache-mapping>

    <cache-mapping>
      <cache-name>near-extend*</cache-name>
      <scheme-name>example-near-extend</scheme-name>
    </cache-mapping>
  </caching-scheme-mapping>

  <caching-schemes>
    <remote-cache-scheme>
      <scheme-name>example-remote</scheme-name>
      <service-name>TcpProxyService</service-name>
    </remote-cache-scheme>

    <view-scheme>
      <scheme-name>example-view-extend</scheme-name>
      <service-name>ViewExtendDistributedCache</service-name>
      <back-scheme>
        <remote-cache-scheme>
          <scheme-ref>example-remote</scheme-ref>
        </remote-cache-scheme>
      </back-scheme>
    </view-scheme>

    <near-scheme>
      <scheme-name>example-near-extend</scheme-name>
      <service-name>NearExtendDistributedCache</service-name>
      <front-scheme>
        <local-scheme/>
      </front-scheme>
      <back-scheme>
        <remote-cache-scheme>
          <scheme-ref>example-remote</scheme-ref>
        </remote-cache-scheme>
      </back-scheme>
    </near-scheme>
  </caching-schemes>
</cache-config>