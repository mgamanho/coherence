<socket-provider>
  <ssl>
    <identity-manager>
      <key-store>
        <url>file:server.jks</url>
        <password>password</password>
      </key-store>
      <password>password</password>
    </identity-manager>

    <trust-manager>
      <algorithm>PeerX509</algorithm>
      <provider>
        <name>CoherenceSecurityProvider</name>
      </provider>
      <key-store>
        <url>file:peer.jks</url>
        <password>password</password>
      </key-store>
    </trust-manager>

    <client-auth>required</client-auth>
  </ssl>
</socket-provider>
